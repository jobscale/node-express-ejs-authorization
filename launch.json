  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "http://127.0.0.1:3000",
      "webRoot": "${workspaceFolder}/docs"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "SLACK_HOST": "https://127.0.0.1:3443"
      },
      "program": "${workspaceFolder}/index.js"
    }
  ]
}